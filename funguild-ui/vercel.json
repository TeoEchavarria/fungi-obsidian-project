{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)\\.wasm",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/wasm"
                }
            ]
        },
        {
            "source": "/(.*)\\.sqlite",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/octet-stream"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}